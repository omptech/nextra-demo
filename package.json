{
  "name": "devsecops-new-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "NODE_ENV=production next build && touch out/.nojekyll",
    "lint": "next lint --max-warnings=0",
    "type-check": "tsc",
    "deploy": "gh-pages -d out -t true",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "js-yaml": "^4.1.0",
    "next": "14.1.0",
    "next-seo": "^6.5.0",
    "nextjs-toploader": "^1.6.6",
    "nextra": "^2.13.2",
    "nextra-theme-docs": "^2.13.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "20.11.19",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "gh-pages": "^6.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "typescript": "5.3.3"
  },
  "lint-staged": {
    "src/**/*.{ts,json,md}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  }
}
